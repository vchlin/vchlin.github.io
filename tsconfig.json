{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "esnext",
    ],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "outDir": "dist",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2015",
    "verbatimModuleSyntax": true
  }
}